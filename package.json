{
  "name": "youtube-clone",
  "version": "0.0.1",
  "description": "cloning youtube with nodejs",
  "main": "src/init.js",
  "repository": "github.com/jaeyeonling/youtube-clone",
  "author": "Jaeyeon Kim <jaeyeonling@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "11.x.x",
    "npm": "6.9.x",
    "yarn": "1.x"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node ./src/init.js --delay 2 --ignore '.scss' --ignore 'static'",
    "dev:assets": "cd src && PROFILE=development webpack -w",
    "build:assets": "cd src && PROFILE=production webpack",
    "build:server": "babel src --out-dir dist --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static dist && cp -R src/views dist",
    "build": "yarn run build:server && yarn run build:assets && yarn run copyAll",
    "prebuild": "rm -rf dist",
    "start": "node dist/init",
    "prestart": "yarn run build",
    "deploy": "git push heroku master",
    "config:push": "heroku config:push --overwrite"
  },
  "devDependencies": {
    "yarn": "^1.17.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/cli": "^7.2.3",
    "@babel/polyfill": "^7.2.5",
    "autoprefixer": "^9.4.9",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "localtunnel": "^1.9.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0"
  },
  "dependencies": {
    "aws-sdk": "^2.433.0",
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "helmet": "^3.15.1",
    "mongoose": "^5.4.16",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "pug": "^2.0.3"
  }
}
